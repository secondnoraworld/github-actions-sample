#!/bin/sh

set -eu

main() {
  merge_commits=$(git --no-pager log --oneline --merges "$MAIN".."$DEVELOP" | awk '{ print $1 }')
  gh api "https://api.github.com/repos/$REPOSITORY/commits/$merge_commits/pulls" --jq .[].number
}

main "$@"

